{
  "name": "xxvcomponent",
  "private": false,
  "version": "1.0.1",
  "main": "./dist/xxvcomponent_umd.js",
  "module": "./dist/xxvcomponent_es.js",
  "types": "./dist/plugin.d.ts",
  "scripts": {
    "dev": "rollup -c ./build/rollup.config.dev.js --watch ",
    "build": "npm run clear && npm run build:es && npm run build:umd",
    "build:es": "rollup --config ./build/rollup.config.es.js",
    "build:umd": "rollup --config ./build/rollup.config.umd.js",
    "lint": "eslint --ext .vue,.ts,.tsx ./src",
    "jest": "jest src --watch",
    "jest:commit": "jest src",
    "clear": "rimraf ./dist",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint & npm run jest:commit"
    }
  },
  "dependencies": {
    "element-plus": "^2.1.4",
    "vue": "^3.2.25"
  },
  "peerDependencies": {
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/user-event": "^13.5.0",
    "@testing-library/vue": "^6.4.2",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/babel-preset-app": "^5.0.3",
    "@vue/test-utils": "^2.0.0-rc.18",
    "babel-jest": "^26.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^4.3.8",
    "jest": "^26.0.0",
    "jest-transform-stub": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.49.9",
    "ts-jest": "^26.4.4",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.29.8"
